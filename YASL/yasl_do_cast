#
3
!= ? (1 exit)

0:=:1 0:!
0:=:2 0:!
0:0 0:+:1 // cast en int

"+":2
2:%== ? (
2:!
2:!
//"plus\n":2 2:print
1:+:1
1:print
"\n":1 1:print
0:1 1:exit
)

2:!
"-":2
2:%== ? (
2:!
2:!
//"moins\n":2 2:print
1:-:1
1:print
"\n":1 1:print
0:1 1:exit
)

2:!
"*":2
2:%== ? (
2:!
2:!
//"multiply\n":2 2:print
1:*:1
1:print
"\n":1 1:print
0:1 1:exit
)

2:!
"/":2
2:%== ? (
2:!
2:!
//"divide\n":2 2:print
1:/:1
1:print
"\n":1 1:print
0:1 1:exit
)

2:!
"%":2
2:%== ? (
2:!
2:!
//"divide\n":2 2:print
1:%:1
1:print
"\n":1 1:print
0:1 1:exit
)

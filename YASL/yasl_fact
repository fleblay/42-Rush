#!ressources/yasl_linux

{
	0:0
	0:%== ? (1:print "\n":1 1:print 0:1 1:exit)
	0:!
	0:=:1
	1:*:1
	-1:0
	0:+:0
	recurs
}recurs

!
!

#
1
!= ? ("Wrong arg number\n" print 1 exit)

0:0 0:+
0
%< ? ("First arg must be positive\n" print 1 exit)
%== ? ("1\n" print 0 exit)

! // j'ai mon nombre sur 0
1:1 // j'ai un 1 sur 1

recurs
